{
  "manifest_version": 2,

  "name": "projinks",
  "description": "Links for my project == projinks",
  "version": "0.0.1",
  "permissions":[
  	"tabs",
    "storage",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches" : ["http://*/*"],
      "js" : ["js/jquery.js","js/myscripts.js"]
    }
  ],
  "icons": { 
    "16" : "images/pj_icon16.png",
    "48" : "images/pj_icon48.png",
    "128" : "images/pj_icon128.png" 
  },

  "browser_action": {
    "default_icon": "images/pj_icon16.png",
    "default_popup": "popup.html"
  }
}