{
  "manifest_version": 2,

  "name": "projinks",
  "description": "Links for my project == projinks",
  "version": "1.0",
  "permissions":[
  	"tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches" : ["http://*/*"],
      "js" : ["js/jquery.js","js/myscripts.js"]
    }
  ],

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}