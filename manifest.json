{
  "manifest_version": 2,

  "name": "Projinks (Beta)",
  "description": "Store multiple links in projinks",
  "version": "0.1.3",
  "permissions":[
  	"tabs",
    "storage",
    "clipboardWrite"
  ],
  "icons": { 
    "16" : "images/pj_icon16.png",
    "48" : "images/pj_icon48.png",
    "128" : "images/pj_icon128.png" 
  },

  "browser_action": {
    "default_icon": "images/pj_icon16.png",
    "default_popup": "popup.html"
  }
}
